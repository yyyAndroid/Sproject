<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomCharView">
        <attr name="drawColor" format="color"/>
    </declare-styleable>
    
    <declare-styleable name="MoveRecorderView">
        <attr name="swidth" format="integer"/>
        <attr name="sheight" format="integer"/>
        <attr name="is_open_camera" format="boolean"/>
        <attr name="record_max_time" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>
</resources>